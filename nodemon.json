{
  "watch": ["server", "client", "public", "package.json"],
  "ext": "js,mjs,cjs,json",
  "ignore": [
    "node_modules",
    "server/db/**",
    "**/*.sqlite",
    "**/*.sqlite3",
    "**/*.db",
    "**/*.db-shm",
    "**/*.db-wal",
    "**/*.db-journal",
    "logs/**"
  ],
  "exec": "node server/app.js"
}
